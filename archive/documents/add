<?xml version="1.0" ?>
<html>
    <head>
        <title>Add</title>
        <script id="script">
['+'] = function() {
    var add = window.prompt("Add");
    if (add) {
        var element = document.createElement("iframe");
        element.src = add;
        document.body.appendChild(element);
    }
}
        </script>
    </head>

    <body><a href="javascript:['+']()">+</a></body>
</html>

