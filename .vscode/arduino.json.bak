{
    "board": "m5stack:esp32:m5stack-timer-cam",
    "port": "/dev/ttyS3",
    "buildPreferences": [
        [
            "build.extra_flags",
            "-DESP32 -DBOARD_HAS_PSRAM -g -DDEBUG"
        ],
        [
            "compiler.cpp.extra_flags",
            "-fmax-errors=4 -Ofast -g -DDEBUG"
        ]
    ],
    "sketch": "Arduino/feebee-cam/feebee-cam.ino",
    "output": "../build",
    "configuration": "PSRAM=enabled,PartitionScheme=default,CPUFreq=240,UploadSpeed=1500000,DebugLevel=none"
}