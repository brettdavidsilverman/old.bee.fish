   "version": "0.2.0",
   "configurations": [
   {
      "type": "by-gdb",
      "request": "launch",
      "name": "Launch(gdb)",
      "program": "${fileBasenameNoExtension}",
      "cwd": "${workspaceRoot}"
   },
      {
         "name": "(gdb) Launch",
         "type": "cppdbg",
         "request": "launch",
         "program": "/home/bee/bee.fish/server/json/json",
         "args": [
            "-test"
         ],
         "stopAtEntry": false,
         "cwd": "${fileDirname}",
         "environment": [],
         "externalConsole": false,
         "MIMode": "gdb",
         "setupCommands": [
            {
               "description": "Enable pretty-printing for gdb",
               "text": "-enable-pretty-printing",
               "ignoreFailures": true
            }
         ],
         "miDebuggerPath": "/usr/bin/gdb"
      },
      {
         "type": "pwa-msedge",
         "request": "launch",
         "name": "Launch Edge against localhost",
         "url": "https://laptop",
         "webRoot": "${workspaceFolder}"
      }
   ]
}