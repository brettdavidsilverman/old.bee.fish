{
    "port": "/dev/ttyS3",
    "buildPreferences": [
        [
            "build.extra_flags",
            "-DESP32 -DBOARD_HAS_PSRAM"
        ],
        [
            "compiler.cpp.extra_flags",
            "-fmax-errors=4 -Ofast"
        ]
    ],
    "prebuild": "./pre-build.sh",
