{
    "port": "/dev/ttyS3",
    "buildPreferences": [
        [
            "build.extra_flags",
            "-D ESP32 -D BOARD_HAS_PSRAM"
        ],
        [
            "compiler.cpp.extra_flags",
            "-f max-errors=4 -O fast"
        ]
    ],
    "prebuild": "./pre-build.sh",
    "postbuild": "./post-build.sh",
    "configuration": "PSRAM=enabled,PartitionScheme=no_ota,CPUFreq=240,UploadSpeed=1500000,DebugLevel=debug",
    "board": "m5stack:esp32:m5stack-timer-cam",
    "sketch": "feebee-cam/feebee-cam.ino",
    "output": "../build"
}