{
    "board": "m5stack:esp32:m5stack-timer-cam",
    "port": "/dev/ttyS3",
    "buildPreferences": [
        [
            "build.extra_flags",
            "-DESP32 -DBOARD_HAS_PSRAM"
        ],
        [
            "compiler.cpp.extra_flags",
            "-fmax-errors=4 -Ofast -w"
        ]
    ],
    "sketch": "Arduino/web-server/web-server.ino",
    "output": "../build",
    "configuration": "PSRAM=enabled,PartitionScheme=default,CPUFreq=240,UploadSpeed=1500000,DebugLevel=debug"
}