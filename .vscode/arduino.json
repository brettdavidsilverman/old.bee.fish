{
    "port": "/dev/ttyS3",
    "buildPreferences": [
        [
            "build.extra_flags",
            "-DESP32 -DBOARD_HAS_PSRAM -DDEBUG"
        ],
        [
            "compiler.cpp.extra_flags",
            "-fmax-errors=4 -Ofast -w -DDEBUG"
        ]
    ],
    "prebuild": "./pre-build.sh",
    "postbuild": "./post-build.sh",
    "configuration": "PSRAM=enabled,PartitionScheme=default,CPUFreq=240,UploadSpeed=1500000,DebugLevel=debug",
    "board": "m5stack:esp32:m5stack-timer-cam",
    "sketch": "Arduino/rtc/rtc.ino",
    "output": "../build"
}