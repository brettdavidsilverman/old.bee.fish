
(rules PCB beehive
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (app.freerouting.autoroute on)
    (postroute on)
    (vias off)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 655)
    (layer_rule B.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.3)
    )
  )
  (rule
    (width 250.0)
    (clear 400.0)
    (clear 125.0 (type smd_to_turn_gap))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" default
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer B.Cu)
    )
  )
  (class "kicad_default"
    +5V "Net-(D1-Pad1)" SCL SDA GND "Net-(Q1-Pad2)" RedLight "Net-(D0-Pad1)"
    "Net-(Q0-Pad2)" WhiteLight "Net-(D2-Pad1)" "Net-(D3-Pad1)" "Net-(D4-Pad1)" "Net-(D5-Pad1)" "Net-(D6-Pad1)" "Net-(D7-Pad1)"
    "Net-(D8-Pad1)"
    (clearance_class default)
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer B.Cu)
    )
  )
)